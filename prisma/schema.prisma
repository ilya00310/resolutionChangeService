
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Video {
  id  Int   @id   @default(autoincrement())
  video_id String
  processing_stage ProcessingProcess @default(unprocessed)
  processing_success ProcessingSuccess @default(null)
  @@unique([video_id])
}

enum ProcessingProcess {
  unprocessed
  compressing
  finished
}

enum ProcessingSuccess {
  null
  true
  false
}